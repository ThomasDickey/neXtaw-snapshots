!This can be useful if you have an IntelliMouse.  It maps the button
!events generated by the wheel
*VT100*translations:	#override \
	!Ctrl			<Btn4Down>:	scroll-back(1,halfpage) \n\
	!Lock Ctrl		<Btn4Down>:	scroll-back(1,halfpage) \n\
	!Lock @Num_Lock Ctrl	<Btn4Down>:	scroll-back(1,halfpage) \n\
	! @Num_Lock Ctrl	<Btn4Down>:	scroll-back(1,halfpage) \n\
				<Btn4Down>:	scroll-back(5,line) \n\
	!Ctrl			<Btn5Down>:	scroll-forw(1,halfpage) \n\
	!Lock Ctrl		<Btn5Down>:	scroll-forw(1,halfpage) \n\
	!Lock @Num_Lock Ctrl	<Btn5Down>:	scroll-forw(1,halfpage) \n\
	! @Num_Lock Ctrl	<Btn5Down>:	scroll-forw(1,halfpage) \n\
				<Btn5Down>:	scroll-forw(5,line) \n\
	<Key>F1: string("\033[11~") \n\
	<Key>F2: string("\033[12~") \n\
	<Key>F3: string("\033[13~") \n\
	<Key>F4: string("\033[14~") \n\
	!Alt Shift Ctrl<Key>Insert: string("\033\033[2@") \n\
	!Alt Shift<Key>Insert: string("\033\033[2$") \n\
	!Alt Ctrl<Key>Insert: string("\033\033[2^") \n\
	!Shift Ctrl<Key>Insert: string("\033[2@") \n\
	!Alt<Key>Insert: string("\033\033[2~") \n\
	!Shift<Key>Insert: insert-selection(PRIMARY, CUT_BUFFER0) \n\
	!Ctrl<Key>Insert: string("\033[2^") \n\
	<Key>Insert: string("\033[2~") \n\
	!Alt Shift Ctrl<Key>Delete: string("\033\033[3@") \n\
	!Alt Shift<Key>Delete: string("\033\033[3$") \n\
	!Alt Ctrl<Key>Delete: string("\033\033[3^") \n\
	!Shift Ctrl<Key>Delete: string("\033[3@") \n\
	!Shift<Key>Delete: string("\033[3$") \n\
	!Ctrl<Key>Delete: string("\033[3^") \n\
	!Alt<Key>Delete: string("\033\033[3~") \n\
	<Key>Delete: string("\033[3~") \n\
	!Alt Shift Ctrl<Key>Home: string("\033\033[7@") \n\
	!Alt Shift<Key>Home: string("\033\033[7$") \n\
	!Alt Ctrl<Key>Home: string("\033\033[7^") \n\
	!Shift Ctrl<Key>Home: string("\033[7@") \n\
	!Shift<Key>Home: string("\033[7$") \n\
	!Ctrl<Key>Home: string("\033[7^") \n\
	!Alt<Key>Home: string("\033\033[7~") \n\
	<Key>Home: string("\033[7~") \n\
	!Alt Shift Ctrl<Key>End: string("\033\033[8@") \n\
	!Alt Shift<Key>End: string("\033\033[8$") \n\
	!Alt Ctrl<Key>End: string("\033\033[8^") \n\
	!Shift Ctrl<Key>End: string("\033[8@") \n\
	!Shift<Key>End: string("\033[8$") \n\
	!Ctrl<Key>End: string("\033[8^") \n\
	!Alt<Key>End: string("\033\033[8~") \n\
	<Key>End: string("\033[8~") \n\
	!Alt Shift Ctrl<Key>Prior: string("\033\033[5@") \n\
	!Alt Shift<Key>Prior: string("\033\033[5$") \n\
	!Alt Ctrl<Key>Prior: string("\033\033[5^") \n\
	!Shift Ctrl<Key>Prior: string("\033[5@") \n\
	!Shift<Key>Prior: scroll-back(1,halfpage) \n\
	!Ctrl<Key>Prior: string("\033[5^") \n\
	!Alt<Key>Prior: string("\033\033[5~") \n\
	<Key>Prior: string("\033[5~") \n\
	!Alt Shift Ctrl<Key>Next: string("\033\033[6@") \n\
	!Alt Shift<Key>Next: string("\033\033[6$") \n\
	!Alt Ctrl<Key>Next: string("\033\033[6^") \n\
	!Shift Ctrl<Key>Next: string("\033[6@") \n\
	!Shift<Key>Next: scroll-forw(1,halfpage) \n\
	!Ctrl<Key>Next: string("\033[6^") \n\
	!Alt<Key>Next: string("\033\033[6~") \n\
	<Key>Next: string("\033[6~") \n\
	!Ctrl<Key>Left: string("\033[d") \n\
	!Ctrl<Key>Right: string("\033[c")

