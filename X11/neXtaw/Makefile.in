#
# Standard .c to .o compile line.
#
.SUFFIXES: .c .i $o
.c.i :
	@RULE_CC@
	@ECHO_CC@$(CPP) $(CPPFLAGS) -c $< >$@
.c$o :
	@RULE_CC@
	@ECHO_CC@$(LIBTOOL_COMPILE) $(CC) $(CFLAGS) $(EXTRA_CFLAGS) $(CPPFLAGS) -c $<

